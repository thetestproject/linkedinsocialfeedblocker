{
  "manifest_version": 3,
  "name": "LinkedIn Social Feed Blocker",
  "version": "1.0.1",
  "description": "Blocks the LinkedIn Social Feed at linkedin.com/feed/.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
	"48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/feed/*"],
	  "css": ["css/hide.css"]
    }
  ],
   "browser_specific_settings": {
  "gecko": {
    "id": "@0dc173d4-ecc4-4c1b-befb-23bfd4ce6be2",
    "strict_min_version": "58.0",
	"data_collection_permissions": {
      "required": ["none"]
    }
  }
}  
}


